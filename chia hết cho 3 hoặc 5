#include <bits/stdc++.h>

using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin >> n;
    long long tong = 0;
    for(int i=3;i<n;i+=3){
        tong+= i;
    }
    for(int i=5;i<n;i+=5){
        tong+= i;
    }
    for(int i=15;i<n;i+=15){
        tong-= i;
    }
    cout << tong;

    return 0;
}
